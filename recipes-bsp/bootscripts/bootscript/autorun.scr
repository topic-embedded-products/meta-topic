setenv devtype mmc
setenv distro_bootpart 2
setenv devnum 0  
setenv fdt_addr ${devicetree_addr}
load ${devtype} ${devnum}:${distro_bootpart} ${fdt_addr} /boot/system.dtb
load ${devtype} ${devnum}:${distro_bootpart} $kernel_addr /boot/uImage
setenv bootargs console=ttyPS0,115200 root=/dev/mmcblk${devnum}p${distro_bootpart} rw rootwait quiet
bootm $kernel_addr - $fdt_addr
