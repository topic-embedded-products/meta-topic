export BB_ENV_EXTRAWHITE=MACHINE
export BUILDDIR="${PWD}"
TOPDIR=`dirname ${PWD}`
export PATH=${TOPDIR}/oe-core/scripts:${TOPDIR}/bitbake/bin:$PATH
